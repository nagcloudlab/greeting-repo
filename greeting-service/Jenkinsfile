pipeline {
    agent any
    stages {
        stage('Compile') {
            steps {
                bat './greeting-service/mvnw clean compile'
            }
        }
        stage('Unit-Testing') {
            steps {
                bat './greeting-service/mvnw test'
            }
        }
        stage('Integration') {
            steps {
                echo 'Integration Testing'
            }
        }
        stage('Package') {
            steps {
                echo './greeting-service/mvnw package'
            }
        }
    }
}